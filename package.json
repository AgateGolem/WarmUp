{
  "name": "mern-course",
  "version": "1.0.0",
  "description": "Mern stack",
  "main": "app.js",
  "scripts": {
    "start": "npm run server",
    "heroku-postbuild": "cd client && npm i && npm run start",
    "server": "npx kill-port 5000 && nodemon app.js",
    "client": "npx kill-port 3000 && npm run start --prefix client",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\""
  },
  "keywords": [
    "mern",
    "react"
  ],
  "dependencies": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "bcryptjs": "^2.4.3",
    "config": "^3.2.4",
    "express": "^4.17.1",
    "express-validator": "^6.3.0",
    "jsonwebtoken": "^8.5.1",
    "materialize-css": "^1.0.0-rc.2",
    "mongoose": "^5.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "nodemon": "^2.0.2",
    "react-scripts": "^4.0.3",
    "web-vitals": "^2.0.1"
  }
}
